        "line-l": { 
            "default": "tab/left-line.png", 
            "down": "tab/active-left-line.png",
            "hover": "tab/hover-left-line.png"
        }
    },
    "buttonred": {
        "bgPadding": 6,
        "bg": { "default": "button/red/background.png" },
