{
    "__config__": {
        "name": "h5client",
        "resources": [
            "./h5client-spritesheet.json",
            "../fonts/habbo.fnt",
            "../fonts/volter.fnt"
        ]
    },

    "default": {
        "scale": 0.5,
        "bg": " ",
        "corner": " ",
        "line": " ",
        "font": {
            "size": "13px",
            "family": "Arial",
            "color": "black"
        }
    },
    "Window": {
        "padding": 11,
        "bgPadding": 11,
        "bg": "window/background.png",
        "corner-tl": { "default": "window/top-left-corner.png" },
        "corner-tr": { "default": "window/top-right-corner.png" },
        "corner-bl": { "default": "window/bottom-left-corner.png" },
        "corner-br": { "default": "window/bottom-right-corner.png" },
        "line-t": { "default": "window/top-line.png" },
        "line-r": { "default": "window/right-line.png" },
        "line-b": { "default": "window/bottom-line.png" },
        "line-l": { "default": "window/left-line.png" }
    },
    "windowheader": {
        "padding": 11,
        "bgPadding": 11,
        "bg": "window/header/background.png",
        "font": {
            "size": "16px",
            "color": "white",
            "fontWeight": "bold"
        },
        "corner-tl": { "default": "window/header/top-left-corner.png" },
        "corner-tr": { "default": "window/header/top-right-corner.png" },
        "corner-bl": { "default": "window/header/bottom-left-corner.png" },
        "corner-br": { "default": "window/header/bottom-right-corner.png" },
        "line-t": { "default": "window/header/top-line.png" },
        "line-r": { "default": "window/header/right-line.png" },
        "line-b": { "default": "window/header/bottom-line.png" },
        "line-l": { "default": "window/header/left-line.png" }
    },
    "mainmenu": {
        "image": "mainmenu/background.png",
        "header": {
            "image": "mainmenu/top.png",
            "position": {
                "x": 0,
                "y": 0
            },

            "width": 70,
            "height": 79
        },

        "position": {
            "x": 0,
            "y": 0
        },

        "width": 72
    },
    "Button": {
        "bgPadding": 7,
        "bg": { 
            "default": "tab/background.png", 
            "down": "tab/active-background.png",
            "hover": "tab/hover-background.png"
         },
        "corner-tl": { 
            "default": "tab/top-left-corner.png", 
            "down": "tab/active-top-left-corner.png",
            "hover": "tab/hover-top-left-corner.png"
