    "buttonnavigator": {
        "scale": 1,
        "image": {
            "default": "mainmenu/buttons/navigator.png",
            "hover": "mainmenu/buttons/navigator_hover.png",
            "down": "mainmenu/buttons/navigator_active.png"
        },
        "height": 33
