    "Checkbox": {
        "bgPadding": 4,
        "bg": "checkbox/background.png",
        "corner": { 
            "default": "checkbox/corner.png",
            "checked": "checkbox/active-corner.png"
        },
        "line": { 
            "default": "checkbox/line.png",
            "checked": "checkbox/active-line.png"
        },
        "checkmark": "checkbox/checkmark.png"
    },
