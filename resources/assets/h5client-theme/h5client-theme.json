        "corner-br": { 
            "default": "tab/bottom-right-corner.png", 
            "down": "tab/active-bottom-right-corner.png",
            "hover": "tab/hover-bottom-right-corner.png"
        },
        "line-t": { 
            "default": "tab/top-line.png", 
